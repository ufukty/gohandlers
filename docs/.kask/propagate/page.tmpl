{{define "sitemap-item"}}
{{with .}}
<ul>
  {{if ne .Href ""}}
  <a href="{{.Href}}">{{.Title}}</a>
  {{else}}
  <span>{{.Title}}</span>
  {{end}}
  {{range .Children}}
  <li>{{template "sitemap-item" .}}</li>{{end}}
</ul>
{{end}}
{{end}}

{{define "toc-item"}}
{{if gt .Level 0}}<a href="#{{.ID}}" class="toc{{.Level}}">{{.Title}}</a>{{end}}
{{with .Children}}
<ul>
  {{range .}}
  <li>{{template "toc-item" .}}</li>{{end}}
</ul>{{end}}
{{end}}

